    },
    "scripts": {
        "all": "concurrently 'npm start' 'cd ../../gro-create-case/frontend && npm start' 'cd ../../gro-dashboard/frontend && npm start'",
        "start": "webpack serve --config ./config/webpack.config.cjs",
        "build": "webpack --config ./config/webpack.config.cjs",
        "serve": "serve dist -p 3001",
        "clean": "rm -rf dist",
        "test": "jest --config ./config/jest.config.cjs --coverage",
        "prod": "npm run build && node server.js",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "test:related": "jest --bail --findRelatedTests",
        "test:watch": "jest --watch --config ./config/jest.config.cjs",
        "prepare": "is-ci || husky install config/husky",
        "plop": "plop"
    },
